        "src": "/api/(.*)", 
        "dest": "server.js" 
      },
      {
        "src": "/(.*)", 
        "dest": "server.js" 
      }
    ],
    "env": {
      "ORACLE_DB_USER": "hr", 
      "ORACLE_DB_PASSWORD": "123", 
      "ORACLE_DB_CONNECT_STRING": "localhost/xe" 
    }
  }
  